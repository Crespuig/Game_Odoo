<odoo>
    <data>
        <!-- PLAYERS -->
        <record id="game.player_01" model="res.partner">
            <field name="name">Patriota</field>
            <field name="level">30</field>
            <field name="photo"></field>
        </record>

        <!-- ARCHIPIELAGOS -->
        <record id="game.archipielago_01" model="game.archipielago">
            <field name="name">Mogambo</field>
            <field name="photo"></field>
            <!-- Many2Many -->
            <field name="players" eval="[(6,0,[ref('game.player_01')])]"></field>
        </record>


        <!-- ISLAS -->
        <record id="game.isla_01" model="game.isla">
            <field name="name">Skypea</field>
            <field name="photo"></field>
            <field name="madera">598</field>
            <field name="oro">471</field>
            <field name="bronce">359</field>
            <field name="hierro">347</field>
            <field name="plata">258</field>
            <field name="oro">221</field>
            <field name="adamantium">158</field>
            <field name="level">24</field>
            <!-- Many2One -->
            <field name="archipielago" ref="game.archipielago_01"></field>
            <field name="player" ref="game.player_01"></field>
        </record>
        <record id="game.isla_02" model="game.isla">
            <field name="name">Dressrosa</field>
            <field name="photo"></field>
            <field name="madera">598</field>
            <field name="oro">471</field>
            <field name="bronce">359</field>
            <field name="hierro">347</field>
            <field name="plata">258</field>
            <field name="oro">221</field>
            <field name="adamantium">158</field>
            <field name="level">24</field>
            <field name="archipielago" ref="game.archipielago_01"></field>
            <field name="player" ref="game.player_01"></field>
        </record>
    </data>
</odoo>